[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts\shell\Attributes\shell\System]
@="System"

[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts\shell\Attributes\shell\System\command]
@="pwsh.exe -file \"D:\\Education\\Projects\\MyProjects\\Shell-Scripts\\Attributes\\Change-Attributes.ps1\" \"%1\" \"System\""


[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts\shell\Attributes\shell\Hidden]
