Windows Registry Editor Version 5.00

[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts]
"MUIVerb"="Scripts"
"SubCommands"=""

[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts\shell]

[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts\shell\Attributes]
"MUIVerb"="Attributes"
"SubCommands"=""

[HKEY_CURRENT_USER\Software\Classes\*\shell\Scripts\shell\Attributes\shell]
