Windows Registry Editor Version 5.00

[HKEY_CLASSES_ROOT\Directory\shell\Scripts]
"MUIVerb"="Scripts"
"SubCommands"=""

[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell]

[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell\Youtube]
"MUIVerb"="Youtube"
"SubCommands"=""

[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell\Youtube\shell]

[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell\Youtube\shell\Download Single Video]
@="Download Single Video"
"Icon"=""

[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell\Youtube\shell\Download Single Video\command]
@="powershell.exe -file \"D:\\Programs\\Shell-Scripts\\Youtube\\yt-downloader-single-video.ps1\" \"%1\" \"Remove\"" 


[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell\Youtube\shell\Download Playlist]
@="Download Playlist"
"Icon"=""

[HKEY_CLASSES_ROOT\Directory\shell\Scripts\shell\Youtube\shell\Download Playlist\command]
@="powershell.exe -file \"D:\\Programs\\Shell-Scripts\\Youtube\\yt-downloader-playlist.ps1\" \"%1\""
