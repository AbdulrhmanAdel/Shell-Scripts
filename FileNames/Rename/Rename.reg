"Icon"=""

[HKEY_CURRENT_USER\Software\Classes\Directory\shell\Scripts\shell\Rename\shell\Replace Text\command]
@="pwsh.exe -file \"D:\\Education\\Projects\\MyProjects\\OS\\Windows\\Shell-Scripts\\FileNames\\Rename\\Rename.ps1\" \"%1\"  \"Replace\""
